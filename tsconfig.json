/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2020",
    "module": "es2020",
    "lib": [
      "es2020",
      "dom"
    ],
    "paths": {
"@app/*": [
        "src/app/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@environments/*": [
        "src/environments/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@core/*": [
        "src/app/core/*"
      ],
      "@modules/*": [
        "src/app/modules/*"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@models/*": [
        "src/app/models/*"
      ],
      "@utils/*": [
        "src/app/utils/*"
      ],
      "@components/*": [
        "src/app/components/*"
      ],
      "@directives/*": [
        "src/app/directives/*"
      ],
      "@pipes/*": [
        "src/app/pipes/*"
      ],
      "@guards/*": [
        "src/app/guards/*"
      ],
      "@resolvers/*": [
        "src/app/resolvers/*"
      ],
      "@interceptors/*": [
        "src/app/interceptors/*"
      ],
      "@validators/*": [
        "src/app/validators/*"
      ],
      "@constants/*": [
        "src/app/constants/*"
      ],
      "@enums/*": [
        "src/app/enums/*"
      ],
      "@interfaces/*": [
        "src/app/interfaces/*"
      ],
      "@types/*": [
        "src/app/types/*"
      ],
      "@store/*": [
        "src/app/store/*"
      ],
      "@actions/*": [
        "src/app/store/actions/*"
      ],
      "@effects/*": [
        "src/app/store/effects/*"
      ],
      "@reducers/*": [
        "src/app/store/reducers/*"
      ],
      "@selectors/*": [
        "src/app/store/selectors/*"
      ],
      "@states/*": [
        "src/app/store/states/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  },
}
